min.js\" integrity=\"sha384-fQybjgWLrvvRgtW6bFlB7jaZrFsaBXjsOMm/tB9LTS58ONXgqbR9W8oWht/amnpF\" crossorigin=\"anonymous\"></script>\r\n\r\n<script lang=\"ts\">\r\nimport { Component, Prop, Vue } from 'vue-property-decorator';\r\n@Component\r\nexport default class MenuAdd extends Vue created(){\r\n    created(){\r\n        document.title = \"Restaurateur - Ajouter un menu\"\r\n    }\r\n  }\r\n</script>\r\n<style>\r\n  @import '@/assets/css/AddMenu.css';\r\n</style>\r\n<script>\r\nimport MenuService from \"@/services/MenuService.js\";\r\n\r\nimport JQuery from 'jquery'\r\nwindow.$ = JQuery\r\n\r\n\r\nlet data = new FormData();  \r\nexport default {\r\n  name: \"MenuAdd\",\r\n  data() {\r\n    return {\r\n      menu: {\r\n        id: null,\r\n        name: \"\",\r\n        price: \"\",\r\n        picture: \"\",\r\n        published: false\r\n      },\r\n      submitted: false\r\n    };\r\n  },\r\n  methods: {\r\n       \r\n    saveMenu() {\r\n    data.append('name', this.menu.name);\r\n    data.append('price', this.menu.price);\r\n    \r\n      MenuService.post(data)\r\n        .then(response => {\r\n          this.menu.id = response.data.id;\r\n          console.log(response.data);\r\n           $('#popupsuccess').modal('toggle');\r\n          this.submitted = true;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    \r\n    newMenu() {\r\n      this.submitted = false;\r\n      this.menu = {};\r\n    },\r\n    handleFileUpload(){\r\n    this.picture = this.$refs.picture.files[0];\r\n    console.log(this.picture);\r\n    data.append('picture', this.picture);\r\n  }\r\n  }\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}